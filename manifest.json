{
  "name": "Expandshare Title Script",
  "description": "Adds prefix to ExpandShare tabs. ('local - ', etc.)",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting", 
    "tabs"
  ],
  "host_permissions": [
    "http://titan.local.expandshare.com/*",
    "https://titan.local.expandshare.com/*",
    "https://titan.dev.expandshare.com/*",
    "https://app.expandshare.com/*"
  ],
  "action": {
    "default_icon": {
      "16": "/images/es_logo_16.png",
      "32": "/images/es_logo_32.png",
      "48": "/images/es_logo_48.png",
      "128": "/images/es_logo_128.png"
    }
  },
  "icons": {
    "16": "/images/es_logo_16.png",
    "32": "/images/es_logo_32.png",
    "48": "/images/es_logo_48.png",
    "128": "/images/es_logo_128.png"
  }
}
